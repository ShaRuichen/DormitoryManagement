@{
    ViewData["Title"] = "查看未缴费学生名单";
    ViewData["Ref"] = "manager";
}

<table class="table">
    <thread>
        <tr>
            <th class="col">学号</th>
            <th class="col">姓名</th>
            <th class="col">催缴</th>
        </tr>
    </thread>
    <tbody>
        @foreach (var student in ViewBag.Students)
        {
            <tr>
                <th class="row">@student["id"]</th>
                <td>@student["name"]</td>
                <td>@student["charge_urge"]</td>
            </tr>
        }
    </tbody>
</table>
