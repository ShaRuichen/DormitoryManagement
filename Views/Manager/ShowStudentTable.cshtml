@{
    ViewData["Title"] = "查看学生信息";
    ViewData["Ref"] = "manager";
}

<table>
    <thead>
        <tr>
            <th>学号</th>
            <th>姓名</th>
            <th>性别</th>
            <th>学院</th>
            <th>年级</th>
            <th>班级</th>
            <th>宿舍楼</th>
            <th>房间号</th>
            <th>是否开通门禁</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var student in ViewBag.Students)
        {
            <tr>
                <th>@student["id"]</th>
                <td>@student["name"]</td>
                <td>@student["sex"]</td>
                <td>@student["college"]</td>
                <td>@student["stage"]</td>
                <td>@student["class"]</td>
                <td>@student["building"]</td>
                <td>@student["room"]</td>
                <td>@student["authority"]</td>
            </tr>
        }
    </tbody>
</table>
