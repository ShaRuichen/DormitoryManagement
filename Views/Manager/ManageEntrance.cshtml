@{
    ViewBag.Title = "管理门禁权限";
    ViewBag.Ref = "managers";
}

<ol class="breadcrumb">
    <li><a href="/Manager">主页</a><span style="padding:5px">/</span></li>
    <li class="active">管理门禁权限</li>
</ol>
<form class="myFrame" action="/manager/entrancetable" method="post">
    <div class="form-group">
        ID：<input class="form-control" type="text" name="id" /><br />
    </div>
    <div class="form-group">
        是否开通门禁：
        <div class="form-check form-check-inline">
            是<input class="form-check-input" type="radio" name="open" value="是" />
        </div>
        <div class="form-check form-check-inline">
            否<input class="form-check-input" type="radio" name="open" value="否" />
        </div>
        <br />
    </div>
    <input class="btn-block btn-dark" type="submit" value="确定" />
</form>
